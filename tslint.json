{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-angle-bracket-type-assertion": false,
        "quotemark": [ true, "single", "avoid-escape" ],
        "trailing-comma": [ true, {
            "multiline": {
                "objects": "always",
                "arrays": "always",
                "functions": "never",
                "typeLiterals": "ignore"
            }
        }]
    },
    "rulesDirectory": []
}